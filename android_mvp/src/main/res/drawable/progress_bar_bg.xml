<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!--  设置背景色图像资源  -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.33dp" />
            <gradient
                android:startColor="@color/line"
                android:centerX="0.5"
                android:endColor="@color/line"
                android:angle="0"
                />
        </shape>
    </item>

    <!--  设置进度条颜色图像资源  -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.33dp" />
                <gradient
                    android:startColor="@color/color_blue_8296ef"
                    android:centerX="0.5"
                    android:endColor="@color/color_purple_947cf1"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>

</layer-list>
