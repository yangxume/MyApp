<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnTestOkHttp"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="60dp"
        android:text="TestOkHttp"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnTestOkHttpInterceptor"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:text="TestOkHttpInterceptor"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/btnTestOkHttp"
        app:layout_constraintHorizontal_bias="0.212"
        app:layout_constraintStart_toStartOf="@+id/btnTestOkHttp"
        app:layout_constraintTop_toBottomOf="@+id/btnTestOkHttp" />

    <Button
        android:id="@+id/btnTestRetrofit"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:textAllCaps="false"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="20dp"
        android:text="testRetrofit"
        app:layout_constraintEnd_toEndOf="@+id/btnTestOkHttpInterceptor"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="@+id/btnTestOkHttpInterceptor"
        app:layout_constraintTop_toBottomOf="@+id/btnTestOkHttpInterceptor" />
</android.support.constraint.ConstraintLayout>